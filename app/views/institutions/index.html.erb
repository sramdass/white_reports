<div id="main-column">		
	<h1> <%=	title "Institutions Index" %> </h1>
	
	<%= form_tag institutions_path, :method => 'get', :id => "institutions_search" do %>
		 <p>
	    	<%= text_field_tag :search, params[:search] %>
	    	<%= submit_tag "Search", :name => nil %>
	  	</p>
	  	<div id="institutions">
			<%= render 'institutions' %>
		</div>
	<% end %>
	
		
	<%= link_to 'New Institution', new_institution_path %>
	
Date: <input type="text" class='datepicker'>			
</div>

