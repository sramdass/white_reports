<%= form_for student do |f| %>		
	<%= render 'shared/error_messages', :target => student %>
	<div id="form-table-div">
		<p class='heading2'> Edit Student </p>
		<table class="form-table"  summary="Student edit information">
		    <tbody>
				<%= render 'student_fields', :f => f %>
				<%= f.fields_for :student_contact do |builder| %>
					<%= render 'student_contact_fields', :f => builder %>
				<% end %>
				<tr><td class='button' colspan='2'><%= f.submit  "Update Student" %></td></tr>				
			</tbody>
		</table>
	</div>
<% end %>
