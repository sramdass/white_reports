@events=Event.all
<div id="main-column">	
	<p class='heading2'> Events Index </p>
	<% @events.each do |event| %>
		<p> <%= event.name %> &nbsp; -  &nbsp; <%= event.startime %> &nbsp;  -  &nbsp; <%= event.endtime%> </p>
	<% end %>
	
   <%= calendar_for(@d.year,@d.month) do |day| %>
   		<% @events.each do |event| %>
	    	<% if event.is_on?(day) %>
	      		<%= event.name %>
	    	<% else %>
	      		Hello <%= day.day %>
	    	<% end %>
    	<% end %>
    <% end %>
</div>
